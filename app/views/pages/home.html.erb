<h1>Pages#home</h1>
<p>Find me in app/views/pages/home.html.erb</p>

<% @users.each do |user| %>
  <p><%= user.email %> : <%= user.created_at %></p>
<% end  %>

<% @guilds.each do |guild| %>
  <p><%= guild.name %></p>
<% end  %>

<% @discord_users.each do |duser| %>
  <p><%= duser.username %></p>
<% end%>

<% @guild_users.each do |guser| %>
  <p>Guild-user : <%= guser.join_at %></p>
<% end%>

<%= line_chart @guild.guild_users.group_by_week(:join_at).count %>


