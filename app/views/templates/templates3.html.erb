<body>


<div class="navbar navbar-expand-sm navbar-light navbar-lewagon">
  <div class="container-fluid">


    <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>

      <ul class="navbar-nav me-auto">
        <%# if user_signed_in? %>
          <li class="nav-item">
            <%= link_to leaderboards_path do %>
            <h5 style="margin-top: 8px; margin-right: 20px; font-weight: 900; color: #D25C5C">ü¶Å Safary</h5>
            <% end %>
          </li>

          <div style="width: 400px; margin-left: 50px;">
            <%= form_tag onboardings_path, style: "display: flex; justify-content: center; text-align: center;", method: :get do %>
              <%= text_field_tag :query,
                params[:query],
                class: "form-control",
                placeholder: "Search use case, templates, projects..."
              %>
              <%= submit_tag "üîç Search", class: "edit-button1" %>
            <% end %>
          </div>


          <li class="nav-item">
            <%= link_to "Use cases", use_cases_path, class: "nav-link" %>
          </li>
          <li class="nav-item">
            <%= link_to "Templates", templates3_path, class: "nav-link", style: "color: black; font-weight: 700" %>
          <li class="nav-item">
            <%= link_to "Audiences", projects_path, class: "nav-link" %>
          </li>
          <li class="nav-item">
            <%= link_to "Explore", dashboards_path, class: "nav-link"%>
          </li>
        <%# else %>
          <!--li class="nav-item"-->
            <%#= link_to "Login", new_user_session_path, class: "nav-link" %>
          <!--/li-->
        <%# end %>
      </ul>

    <div class="collapse navbar-collapse" id="navbarSupportedContent">
      <p><%= link_to "My dashboards", dashboards3_path, class: "nav-link"%></p>
        <%= link_to profile_path do %>
          <% if current_user.photo.attached? %>
             <%= cl_image_tag current_user.photo.key, width: 40, height: 40, :crop=>"fill", style: "border-radius: 100%" %>
            <% else %>
            <%= image_tag("no-photo.png", :width => 30, style: "border-radius: 100%;" ) %>
          <% end %>
      <% end %>
            <%= link_to destroy_user_session_path, method: :delete do  %>
              <i class="fas fa-sign-out" style="color: #687086; margin-left: 10px"></i>
            <% end %>
    </div>
  </div>
</div>

<div class="container-fluid" id="templates-container2">

  <div style="display: flex; justify-content: start;">
      <p style="color: #687086; margin-left: 200px;">Browse +200 templates created by Safary community </p>
      <div class="use-case"><i class="fa-solid fa-arrow-down-wide-short"></i> Sort: Most popular </div>
  </div>

<div style="margin-left: 200px;">
  <div class="templates-list">
    <div class="template-card">
      <h6 style="font-weight: 900">Discord members acquisition</h6>
      <p style="color: #687086; font-size: 12px;">Discord member growth over time</p>
      <%= image_tag("discord-acquisition.png", :width => 150, style: "margin-left: 10px;") %>
    </div>
    <div class="template-card">
      <h6 style="font-weight: 900">Daily active users (wallets)</h6>
      <p style="color: #687086; font-size: 12px;">Wallet retention over time</p>
      <%= image_tag("retention.png", :width => 150, style: "margin-top: 20px; margin-left: 5px") %>

    </div>
    <div class="template-card">
      <h6 style="font-weight: 900;">Funnel <br> Discord > Dapp</h6>
      <p style="color: #687086; font-size: 12px;">Do Discord members convert?</p>
      <%= image_tag("onboarding.png", :width => 150, style: "margin-left: 10px") %>
    </div>
    <div class="template-card">
       <h6 style="font-weight: 900;">Discord <br> power users</h6>
       <p style="color: #687086; font-size: 12px;">The most active members in your Discord</p>
         <%= image_tag("power-user.png", :width => 130, style: "margin-bottom: 5px; margin-left: 10px") %>
    </div>
    <div class="template-card">
       <h6 style="font-weight: 900">Discord <br> activity times</h6>
        <p style="color: #687086; font-size: 12px;">The best times to post in your Discord</p>
        <%= image_tag("calendar.png", :width => 150, style: "margin-left: 5px") %>
    </div>
      <%= link_to dashboards4_path do %>
    <div class="template-card">
      <h6 style="font-weight: 900; color: black">User segmentation (Token) </h6>
      <p style="color: #687086; font-size: 12px;">% of users token holders</p>
      <%= image_tag("token-acquisition.png", :width => 110, style: "margin-top: -25px; margin-left: 20px") %>
    </div>
    <% end %>
    <div class="template-card">
      <h6 style="font-weight: 900">User segmentation (NFT) </h6>
      <p style="color: #687086; font-size: 12px;">% of users NFT holders</p>
      <%= image_tag("token-acquisition.png", :width => 110, style: "margin-top: -25px; margin-left: 20px") %>
    </div>
    </div>
  </div>


  </div>

</div>


